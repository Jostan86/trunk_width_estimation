{
  "name": "width_estimation_jetson",
  "dockerComposeFile": [
    "../../docker/docker-compose-jetson.yml" 
  ],
  "service": "trunk_width_estimation",
  "workspaceFolder": "/home/vscode/trunk_width_estimation",
  "mounts": [
    "source=${localEnv:HOME}${localEnv:USERPROFILE}/.bash_history,target=/home/vscode/.bash_history,type=bind"
  ],
  "shutdownAction": "stopCompose",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-azuretools.vscode-docker",
        "ms-vscode-remote.remote-containers",
        "ms-toolsai.jupyter"
      ]
    }
  },
  "overrideCommand": true
}